// Overloop Schakelaar
rule "Overloop Schakelaar"
when
        Item OverloopZWScene received update
then
      
	switch(OverloopZWScene.state) {
		case 11 : { }
		case 12 : { 
		}
		case 13 : { 
			sendCommand(OverloopAchterScene, 3)
		}
		case 14 : { }
		case 15 : { }
		case 16 : { }
		case 21 : { 
		}
		case 22 : { 
		}
		case 23 : { 
		}
		case 24 : { }
		case 25 : { }
		case 26 : { }
		case 31 : { 
			if( OverloopVoorScene.state > 0 ) {
				sendCommand(OverloopVoorScene, 0)
			} else {
				sendCommand(OverloopVoorScene, 1)
			}
		}
		case 32 : { }
		case 33 : { 
			if( OverloopVoorScene.state != 2 ) {
				sendCommand(OverloopVoorScene, 2)
			} else {
				sendCommand(OverloopVoorScene, 1)
			}

		}
		case 34 : { }
		case 35 : { }
		case 36 : { }
		case 41 : { 
			if( OverloopAchterScene.state > 0 ) {
				sendCommand(OverloopAchterScene, 0)
			} else {
				sendCommand(OverloopAchterScene, 1)
			}
		}
		case 42 : { }
		case 43 : { 
		       if( OverloopAchterScene.state != 2 ) {
				sendCommand(OverloopAchterScene, 2)
			} else {
				sendCommand(OverloopAchterScene, 1)
			}

		}
		case 44 : { }
		case 45 : { }
		case 46 : { }
		default : {  }
        }

end
